generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  user_id  Int    @id @default(autoincrement())
  nama     String
  email    String @unique
  password String
  photo    Bytes?
}

model Keuangan {
  keuangan_id    Int      @id @default(autoincrement())
  jenisTransaksi String
  keterangan     String
  kategori       String
  tanggal        DateTime
  jumlah         Decimal  @db.Decimal(15, 2)
  catatan        String
}
